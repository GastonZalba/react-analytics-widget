{
  "name": "react-analytics-widget",
  "version": "1.2.0",
  "description": "Embed Google Analytics widgets in your React applications",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/revolunet/react-analytics-widget.git"
  },
  "homepage": "https://revolunet.github.io/react-analytics-widget/",
  "bugs": {
    "url": "https://github.com/revolunet/react-analytics-widget/issues"
  },
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "npm-run-all build-css build-js",
    "build-js": "nwb build-react-component --copy-files",
    "clean": "nwb clean-module && nwb clean-demo && nwb clean-css",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "nwb serve-react-demo",
    "test": "nwb test-react",
    "publish": "gh-pages -d demo/dist && git push",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "build-css": "node-sass-chokidar src/styles/src -o src/styles",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/src -o src/styles --watch --recursive --use-polling --polling-interval 1000"
  },
  "peerDependencies": {
    "react": "15.x"
  },
  "devDependencies": {
    "gh-pages": "^1.0.0",
    "mini-css-extract-plugin": "^1.4.1",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "nwb": "^0.25.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-github-corner": "^0.3.0"
  },
  "author": "Julien Bouquillon <julien@bouquillon.com>",
  "license": "MIT",
  "keywords": [
    "react-component",
    "analytics",
    "widget"
  ],
  "dependencies": {}
}
